<!--page/home.wxml-->
<!-- 1.展示轮播图 -->
<swiper class='swiper' id="swiper" circular autoplay interval="3000" duration="300" indicator-dots
        indicator-active-color="#1296db">
  <block wx:for="{{list}}" wx:key="index">
    <swiper-item class="swiper-item" >
      <image src="{{item.image}}" mode="widthFix" class="swiperImg"/>
    </swiper-item>
  </block>
</swiper>